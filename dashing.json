{
    "name": "Phaser 3",
    "package": "phaser3",
    "index": "docs/index.html",
    "selectors": {
        "h1.page-title": [
            {
                "requiretext": "^Namespace:",
                "type": "Namespace",
                "regexp": "^Namespace\\:\\s",
                "replacement": "",
                "matchpath": "docs/(Phaser|Camera|Matter|Spine).*\\.html$"
            },
            {
                "requiretext": "^Class:",
                "type": "Class",
                "regexp": "^Class\\:\\s",
                "replacement": "",
                "matchpath": "docs/(Phaser|Camera|Matter|Spine).*\\.html$"
            }
        ],
        "dl > dt.name > h4": {
            "type": "Member",
            "regexp": "(\\:.+)|(<[^>]*>)",
            "replacement": "",
            "matchpath": "docs/(Phaser|Camera|Matter|Spine).*\\.html$"
        },
        "dl > dt > h4.name": {
            "type": "Method",
            "regexp": "\\(.*\\)|(<[^>]*>)",
            "replacement": "",
            "matchpath": "docs/(Phaser|Camera|Matter|Spine).*\\.html$"
        },
        "h4[id^=\"event:\"]": {
            "type": "Event",
            "matchpath": "docs/(Phaser|Camera|Matter|Spine).*\\.html$"
        }
    },
    "ignore": [],
    "icon32x32": "resources/docset-icon.png",
    "allowJS": true,
    "ExternalURL": "https://photonstorm.github.io/phaser3-docs/"
}
